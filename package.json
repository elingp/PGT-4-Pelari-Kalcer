    "@tanstack/router-plugin": "^1.139.3",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/react-devtools": "^0.8.1",
